{
  "name": "cy-zs-vite",
  "private": true,
  "version": "3.1.3",
  "main": "electron/electron.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/chenyuxingkong/electron-vite"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "electron": "wait-on tcp:3000 && cross-env IS_DEV=true electron .",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"npm run electron\"",
    "electron:build.win": "npm run build && electron-builder --win --dir",
    "electron:build.linux": "npm run build && electron-builder --linux appImage",
    "electron:build.test": "npm run build && electron-builder --dir",
    "electron:build.exe": "npm run build && electron-builder --win",
    "electron:publish": "set GH_TOKEN=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 && npm run build && electron-builder --win --publish always"
  },
  "dependencies": {
    "@element-plus/icons": "^0.0.11",
    "axios": "^0.26.1",
    "cheerio": "^1.0.0-rc.10",
    "electron-log": "^4.4.6",
    "electron-updater": "^5.0.1",
    "element-plus": "^2.1.8",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.3.0",
    "@vue/cli-service": "^5.0.4",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "electron": "^18.0.3",
    "electron-builder": "^22.14.13",
    "electron-packager": "^15.4.0",
    "electron-winstaller": "^5.0.0",
    "iconv-lite": "^0.6.3",
    "sass": "^1.50.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.6.9",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.9.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-loader": "^12.0.0",
    "vue-style-loader": "^3.0.0",
    "vue-tsc": "^0.29.8",
    "wait-on": "^6.0.1",
    "webpack": "^5.72.0"
  },
  "build": {
    "appId": "CY",
    "productName": "CY-ZS",
    "copyright": "Copyright Â© 2021 xc",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "icon": "electron/logo.ico",
      "publish": [
        "github"
      ]
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  }
}
